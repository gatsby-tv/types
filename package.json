{
  "name": "@gatsby-tv/types",
  "description": "Gatsby shared Typescript types library.",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsby-tv/types.git"
  },
  "version": "0.4.14",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "AGPL-3.0",
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "eslint lib",
    "types": "tsc --noEmit",
    "types:watch": "yarn run check -- --watch",
    "clean": "rm -rf dist",
    "build": "yarn run build:tsc && yarn run build:babel",
    "build:tsc": "tsc --emitDeclarationOnly && tscpaths -p tsconfig.json -s lib -o dist",
    "build:babel": "babel lib --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@babel/preset-typescript": "^7.12.1",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "core-js": "3.7.0",
    "eslint": "^7.16.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "core-js": "^3.7.0"
  }
}
